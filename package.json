{
  "name": "emoxy",
  "description": "A wrapper for emojihub api",
  "version": "1.0.0",
  "author": "COD3VIL",
  "homepage": "https://mykenji.vercel.app/projects/emoxy",
  "repository": "https://github.com/michaelnji/emoxy",
  "files": [
    "dist"
  ],
   "types": "./dist/index.d.ts",
  "main": "./dist/emoxy.umd.cjs",
  "module": "./dist/emoxy.js",
  "exports": {
    ".": {
      "import": "./dist/emoxy.js",
      "require": "./dist/emoxy.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest --ui --typecheck --coverage.enabled=true",
    "coverage": "vitest run --coverage &&  vite preview --outDir html"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@types/node": "^20.11.13",
    "@vitest/coverage-v8": "^1.2.2",
    "@vitest/ui": "^1.2.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-banner": "^0.7.1",
    "vite-plugin-dts": "^3.7.2",
    "vite-plugin-remove-console": "^2.2.0",
    "vitest": "^1.2.2"
  },
  "dependencies": {
    "@types/jest": "^29.5.12",
    "axios": "^1.6.7",
    "ky": "^1.2.0",
    "vite-plugin-node-polyfills": "^0.19.0"
  },
  "keywords": ["emojihub", "emoji", "emoxy"],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/michaelnji/emoxy/issues"
  }
}
